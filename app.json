{
  "expo": {
    "name": "Ohayo",
    "slug": "Ohayo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logoFav.png",
    "scheme": "ohayo",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anshul2311.ohayo"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logoFav.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anshul2311.ohayo"
    },
    "splash": {
      "image": "./assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logoFav.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "imageWidth": 190,
          "resizeMode": "contain",
          "backgroundColor": "#fff"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Ohayo to use your location for weather and local news."
        }
      ],
     [
        "expo-notifications",
        {
          "icon": "./assets/images/logoFav.png",
          "color": "#FFFFFF"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c92d98ab-a852-4475-878b-8a685b0f6b3f"
      }
    }
  }
}
